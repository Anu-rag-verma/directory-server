<?xml version="1.0" encoding="ISO-8859-1"?>

<container name="server">

  <classloader>
    <classpath>
      <repository>
        <resource
          id="commons-collections:commons-collections" 
          version="2.1"/>
        <resource
          id="avalon-framework:avalon-framework-api" 
          version="4.1.5"/>
        <resource
          id="avalon-framework:avalon-framework-impl" 
          version="4.1.5"/>
        <resource
          id="ldapd-common:ldapd-common" 
          version="SNAPSHOT"/>
        <resource
          id="ldapd-server:schema-api"
          version="SNAPSHOT"/>
        <resource
          id="ldapd-server:oid-registry"
          version="SNAPSHOT"/>
        <resource
          id="ldapd-server:bootstrap-syntax-manager"
          version="SNAPSHOT"/>
      </repository>
    </classpath>
  </classloader>

  <component name="bootstrap-syntax-manager" 
    class="ldapd.server.schema.syntax.BootstrapSyntaxManager" />
  <component name="oid-registry" 
    class="ldapd.server.schema.OidRegistryImpl" />
  <component
    name="bootstrap-matching-rule-manager"
    class="ldapd.server.schema.matching.BootstrapMatchingRuleManager"/>

</container>
